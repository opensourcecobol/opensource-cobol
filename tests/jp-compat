    ) 5>&1 2>&1 | eval $at_tee_pipe
    at_status=`cat $at_status_file`
    ;;

  47 ) # 47. file-control.at:355: Allow file delete (INDEXED)
    at_setup_line='file-control.at:355'
    at_desc='Allow file delete (INDEXED)'
    $at_quiet $ECHO_N " 47: Allow file delete (INDEXED)                  $ECHO_C"
    at_xfail=no
    (
      echo "47. file-control.at:355: testing ..."
      $at_traceon


$at_traceoff
echo "file-control.at:357: test \$COB_HAS_ISAM = \"yes\" || exit 77"
echo file-control.at:357 >$at_check_line_file
